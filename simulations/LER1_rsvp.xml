<?xml version="1.0"?>
<sessions>
  <!-- High -->
  <session>
    <endpoint>HighRx</endpoint>
    <tunnel_id>1</tunnel_id>
    <paths>
      <!-- Wireless経路 -->
      <path>
        <lspid>100</lspid>
        <bandwidth>700000</bandwidth>
        <route>
          <node>LER1%routerId</node>
          <node>Wireless%routerId</node>
          <node>LER2%routerId</node>
        </route>
        <permanent>true</permanent>
        <color>100</color>
      </path>
      <!-- Wired経路 -->
      <path>
        <lspid>101</lspid>
        <bandwidth>700000</bandwidth>
        <route>
          <node>LER1%routerId</node>
          <node>Wired%routerId</node>
          <node>LER2%routerId</node>
        </route>
        <permanent>true</permanent>
        <color>101</color>
      </path>
    </paths>
  </session>

